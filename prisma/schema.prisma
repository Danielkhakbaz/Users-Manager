generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  id        Int     @id @default(autoincrement())
  name      String
  email     String  @unique
  followers Int     @default(0)
  isActive  Boolean @default(false)
  size      String  @default("medium")
}
